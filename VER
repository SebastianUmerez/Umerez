#include <stdio.h>
#define N 2
struct vehiculo {
    char marca[20];
    char modelo[20];
    int year;
    int kilometraje;
};
int main() {
    struct vehiculo autos[N]; 
    for(int i = 0; i < N; i++) {
        printf("vehiculo[%d] \n", i + 1);
        printf("ingresar la marca: ");
        scanf("%s",autos[i].marca);
        
        printf("ingresar el modelo: ");
        scanf("%s",autos[i].modelo);
        
        do {
            printf("ingresar el aÃ±o: ");
            scanf("%d",&autos[i].year);
            if(autos[i].year < 0) printf("ERROR\n");
        } while(autos[i].year < 0);
        
        do {
            printf("ingresar el kilometraje: ");
            scanf("%d",&autos[i].kilometraje);
            if(autos[i].kilometraje < 0) printf("ERROR\n");
        } while(autos[i].kilometraje < 0);
    }
    for(int i = 0; i < N; i++) {
        printf("\nDatos de los vehiculos[%d] \n", i+1);
        printf("Nombre: %s \n",autos[i].marca);
        printf("Legajo: %s \n",autos[i].modelo);
        printf("Calle: %d \n",autos[i].year);
        printf("Numero: %d \n",autos[i].kilometraje);
    }
    return 0;
}
#include <stdio.h>

struct producto {
    char nombre[30];
    float precio;
    int cantidad;
};

struct producto cargar_producto(void);
void imprimir_producto(struct producto p); 

void cargar_producto(struct producto *p) {
    struct producto prod;
    printf("ingresar el nombre: ");
    scanf("%s",p->nombre);
    printf("ingresar el precio: ");
    scanf("%f",&p->precio);
    printf("ingresar el cantidad: ");
    scanf("%d",&p->cantidad);
    return prod;
}
void imprimir_producto(struct producto p) {
    printf("\nDatos del producto \n");
    printf("Nombre: %s \n",p.nombre);
    printf("Precio: %.2f \n",p.precio);
    printf("Cantidad: %d \n",p.cantidad);
}
int main() {
    struct producto product = cargar_producto();
    imprimir_producto(product);
    return 0;
} //typedef
