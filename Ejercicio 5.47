#include <stdio.h>

int main() {
    int i, numero, mayor;

    // Pedir el primer número fuera del bucle para inicializar "mayor"
    printf("Ingrese el numero (1): ");
    scanf("%d", &numero);
    mayor = numero;

    // Leer los 9 números restantes
    for (i = 2; i <= 10; i++) {
        printf("Ingrese el numero (%d): ", i);
        scanf("%d", &numero);

        if (numero > mayor) {
            mayor = numero;
        }
    }

    // Mostrar el resultado
    printf("El mayor numero ingresado es: %d\n", mayor);

    return 0;
}
