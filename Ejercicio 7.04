#include <stdio.h>

int main() {
    int N, i;

    printf("Ingrese el n√∫mero de elementos del arreglo: ");
    scanf("%d", &N);

    int arreglo[N];

    for(i = 0; i < N; i++) {
        printf("Ingrese el elemento a[%d]: ", i);
        scanf("%d", &arreglo[i]);
        
        // Validar el rango
        while(arreglo[i] < 1 || arreglo[i] > 100) {
            printf("Valor fuera de rango (1-100). Intente de nuevo: ");
            scanf("%d", &arreglo[i]);
        }
    }

    printf("\nLos elementos del arreglo son:\n");
    for(i = 0; i < N; i++) {
        printf("a[%d]: %d\n", i, arreglo[i]);
    }

    return 0;
}
