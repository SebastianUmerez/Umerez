#include <stdio.h>

int main()
{
    int curso;
    char hostia;
    int horario;
    printf("ingrese a√±o de cursado: ");
    scanf(" %d", &curso);
    printf("ingrese especialidad: ");
    scanf(" %c", &hostia);
    printf("ingrese horario de cursado: ");
