#include <stdio.h>

int main() {
    int numero;   // Variable para guardar el número ingresado

    // Pedir el número al usuario
    printf("Ingrese un numero: ");
    scanf("%d", &numero);

    // Verificar si el número está en el rango [10,100]
    if (numero < 10 || numero > 100) {
        // Si está fuera del rango, se informa y el programa termina
        printf("El numero ingresado no esta dentro del rango\n");
    } else {
        // Si está dentro del rango, verificar si es par o impar
        if (numero % 2 == 0) {
            printf("El numero %d es par\n", numero);
        } else {
            printf("El numero %d es impar\n", numero);
        }
    }

    return 0;  // Fin del programa
}
