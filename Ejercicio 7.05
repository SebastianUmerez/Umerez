    int producto = 0;
    int suma;
    printf("Primer vector: \n");
    for(i = 0; i<N; i++){
        printf("ingrese el elemento[%d]: ", i);
        scanf("%d", &a[i]);
    }
    printf("Segundo vector: \n");
    for(i = 0; i<N; i++){
        printf("ingrese el elemento[%d]: ", i);
        scanf("%d", &b[i]);
    }
