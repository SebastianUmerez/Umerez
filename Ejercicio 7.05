#include <stdio.h>
#define N 3
int main() {
    int i;
    int a[N];
    int b[N];
    int producto = 0;
    
    printf("Primer vector: \n");
    for(i = 0; i<N; i++){
        printf("ingrese el elemento[%d]: ", i);
        scanf("%d", &a[i]);
    }
    printf("Segundo vector: \n");
    for(i = 0; i<N; i++){
        printf("ingrese el elemento[%d]: ", i);
        scanf("%d", &b[i]);
    }
    for(i = 0; i<N; i++){
        producto += a[i]*b[i];
    } 
    printf("el producto punto es: %d \n",producto);
}
