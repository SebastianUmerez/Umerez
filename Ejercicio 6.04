#include <stdio.h>
int ret_max(int a, int b) {
    int max;
    if(a>b) max = a;
    else max = b;
    return max;
}
int main() {
    int num1;
    int num2;
    printf("Ingrese dos numeros diferentes: ");
    scanf("%d %d", &num1,&num2);
    int mayor = ret_max(num1,num2);
   printf("El numero %d es el mayor \n",mayor);
    return 0;
}
